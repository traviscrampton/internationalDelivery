
<div class="container">
<h2>All Delivery Requests</h2><hr>
<div class="list_text_size">
    <% @requests.each do |request| %>
      <ul>
        <li> <%= image_tag request.item.itemimage.url(:thumb)%> <%= link_to request.item.itemname, request_path(request) %> || Coming From: <%= request.fromcountry %> || Going To: <%= request.airport %></li>
      </ul>
    <% end %>
  </div>
</div>
