<%= bootstrap_form_for [@request], remote:true, :authenticity_token => true do |f| %>
  <%= f.fields_for :item do |i| %>
    <%= i.text_field :itemname, label:"What Is The Item?" %>
    <%= i.text_area :itemdescription, label:"Item Description" %>
    <%= i.file_field :itemimage, label:"Upload an Image"%>
  <% end %>
<% end %>
